{
  "name": "apex-ui-docs",
  "version": "1.9.4",
  "description": "apex-ui document vuepress quickapp",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "release": "hap release",
    "server": "hap server --watch",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "prettier": "prettier --write \"**/*.ux\""
  },
  "license": "MIT",
