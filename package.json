    "@vuepress/plugin-register-components": "^1.8.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "apex-ui": "latest",
    "clipboard": "^2.0.4",
    "less": "^3.13.1",
    "less-loader": "^7.0.2",
    "prettier": "^1.19.1",
    "prettier-plugin-quickapp": "^0.1.0",
    "vuepress-plugin-feedback": "^1.1.0",
    "vuepress": "^1.8.2"
  },
  "browserslist": [
    "chrome 65"
